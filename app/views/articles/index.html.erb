<div class="articles-container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 ">
      <% @articles.reverse_each do |article| %>
        <div class="article">
          <h1><%= link_to article.title, article_path(article) %></h1>
          <p><%= article.content %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>
